        "@types/node": "^24.1.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@midscene/android": "^0.23.4",
        "@midscene/web": "^0.23.4",
        "dotenv": "^17.2.1",
        "playwright": "^1.54.1"
    }
}