{
    "name": "midscene-test",
    "version": "1.0.0",
    "description": "Midscene testing demo project",
    "main": "examples/demo-test.ts",
    "type": "module",
    "scripts": {
        "test": "tsx examples/demo-test.ts",
        "test:android": "tsx examples/android-test.ts",
        "test:headless": "tsx examples/demo-test.ts --headless",
